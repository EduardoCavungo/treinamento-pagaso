<div class="page-header">
  <%= link_to bancos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_banco_path(@banco), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show banco</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @banco.nome %></dd>

  <dt>Email:</dt>
  <dd><%= @banco.email %></dd>

  <dt>Sigla:</dt>
  <dd><%= @banco.sigla %></dd>

  <dt>Morada:</dt>
  <dd><%= @banco.morada %></dd>

  <dt>Telefone:</dt>
  <dd><%= @banco.telefone %></dd>

  <dt>Web site:</dt>
  <dd><%= @banco.web_site %></dd>

</dl>
